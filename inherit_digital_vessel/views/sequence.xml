<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="sequence_self_perbaikan" model="ir.sequence">
		   <field name="name">Self perbaikan</field>
		   <field name="code">self.perbaikan</field>
		   <field name="active">TRUE</field>
		   <field name="prefix">LP.</field>
		   <field name="suffix">/%(day)s/%(month)s/PPM/JPPI-%(y)s</field>
		   <field name="padding">3</field>
		   <field name="number_next">1</field>
		   <field name="number_increment">1</field>
		</record>

		<record id="sequence_self_kerusakan" model="ir.sequence">
		   <field name="name">Self Kerusakan</field>
		   <field name="code">self.kerusakan</field>
		   <field name="active">TRUE</field>
		   <field name="prefix">LK.</field>
		   <field name="suffix">/%(day)s/%(month)s/PPM/JPPI-%(y)s</field>
		   <field name="padding">3</field>
		   <field name="number_next">1</field>
		   <field name="number_increment">1</field>
		</record>

		<record id="sequence_docking" model="ir.sequence">
		   <field name="name">Self Docking</field>
		   <field name="code">self.docking</field>
		   <field name="active">TRUE</field>
		   <field name="prefix">DO/%(day)s%(month)s%(y)s/</field>
		   <field name="padding">3</field>
		   <field name="number_next">1</field>
		   <field name="number_increment">1</field>
		</record>

		<record id="sequence_weekly" model="ir.sequence">
		   <field name="name">Self Weekly</field>
		   <field name="code">self.weekly</field>
		   <field name="active">TRUE</field>
		   <field name="prefix">WI/%(day)s%(month)s%(y)s/</field>
		   <field name="padding">3</field>
		   <field name="number_next">1</field>
		   <field name="number_increment">1</field>
		</record>

		<record id="sequence_daily" model="ir.sequence">
		   <field name="name">Self Daily</field>
		   <field name="code">self.daily</field>
		   <field name="active">TRUE</field>
		   <field name="prefix">DO/%(day)s%(month)s%(y)s/</field>
		   <field name="padding">3</field>
		   <field name="number_next">1</field>
		   <field name="number_increment">1</field>
		</record>
		
	</data>
</odoo>